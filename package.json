{
	"name": "cronos",
	"displayName": "Cronos",
	"description": "Extension to save changes over any file.",
	"version": "0.0.1",
	"publisher": "Diego",
	"engines": {
		"vscode": "^0.10.1"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCommand:extension.cronos"
	],
	"main": "./out/src/extension",
	"contributes": {
		"commands": [{
			"command": "extension.cronos",
			"title": "Cronos"
		}]
	},
	"scripts": {
		"vscode:prepublish": "node ./node_modules/vscode/bin/compile",
		"compile": "node ./node_modules/vscode/bin/compile -watch -p ./"
	},
	"devDependencies": {
		"typescript": "^1.6.2",
		"vscode": "0.10.x"
	}
}