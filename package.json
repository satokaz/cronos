{
	"name": "cronos",
	"displayName": "Cronos",
	"description": "Will save all the changes that you've made onto your files.",
	"version": "0.0.1",
	"publisher": "Diego",
    "icon":"images/cronos.svg",
    "bugs": {
        "url": "https://github.com/DracotMolver/cronos/issues"
    },
    "homepage":{
        "homepage": "https://github.com/Microsoft/vscode-spell-check/blob/master/README.md"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/DracotMolver/cronos"
    },
	"engines": {
		"vscode": "^0.10.1"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCommand:extension.cronos"
	],
	"main": "./out/src/extension",
	"contributes": {
		"commands": [{
			"command": "extension.cronos",
			"title": "Cronos"
		}]
	},
	"scripts": {
		"vscode:prepublish": "node ./node_modules/vscode/bin/compile",
		"compile": "node ./node_modules/vscode/bin/compile -watch -p ./"
	},
	"devDependencies": {
		"typescript": "^1.6.2",
		"vscode": "0.10.x"
	}
}